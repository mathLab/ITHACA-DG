FoamFile
{
    version     0.1;
    format      ascii;
    class       dictionary;
    location    "system";
    object      dgSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         default average;
    grad(p)         default average;
     grad(U)  default average;
}

divSchemes
{
    default         default average;
    div(U)	    default average;
    div(U,U)	    default LF;
}

laplacianSchemes
{
    default         default 20;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         orthogonal;
}

fluxRequired
{
    default         no;
    p               ;
}


